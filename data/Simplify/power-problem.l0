fun bool main (int a) =
    let x1 = a pow (~2) = 1 / (a*a) in
    let x2 = (a pow ~2) = (1/a) * (1/a) in
    let x3 = a pow 1 = a in
    let x4 = a pow (~3) = (1/a)*(1/a)*(1/a) in
    let x5 = 0 pow 0 = 1  in
    let x6 = a pow 0 = 1  in
    let x7 = 1 / (a pow (~2)) = a pow 2 in
    x1 && x2 && x3 && x4 && x5 && x6 && x7
