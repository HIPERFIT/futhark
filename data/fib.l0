// Read an integer from the user, then compute that number of fibonacci numbers.
fun [int] main() =
    let s = write("Please enter a number: ") in
    let n = read(int) in
    let arr = iota(n) in
    let (res=arr) = for i < n-2 do
                      let x = res[i] in
                      let y = res[i+1] in
                      let res[i+2] = x + y
                      in res
    in write(res)
