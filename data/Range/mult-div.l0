    let mul_t2 = x * y in
    let mul_t3 = xn * yn in
    let mul_t5 = x * yn in
    let mul_t5 = xn * y in

    let div_t0 = x / 2 in
    let div_t1 = x / ~2 in
    let div_t2 = x / y in
    let div_t3 = xn / yn in
    let div_t4 = x / yn in
    let div_t5 = xn / y in

